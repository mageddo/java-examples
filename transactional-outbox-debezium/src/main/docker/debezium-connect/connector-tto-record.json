{
  "name": "pg-tto-record-outbox",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "postgres",
    "database.port": "5432",
    "database.user": "debezium",
    "database.password": "debezium",
    "database.dbname": "appdb",
    "plugin.name": "pgoutput",
    "slot.name": "pg_tto_record_slot",
    "publication.autocreate.mode": "filtered",
    "tombstones.on.delete": "false",
    "decimal.handling.mode": "double",
    "schema.include.list": "public",
    "table.include.list": "public.tto_record",
    "snapshot.mode": "initial",
    "slot.drop.on.stop": "false",
    "topic.prefix": "dbz",
    "include.schema.changes": "false",
    "transforms": "outbox,extract",
    "transforms.outbox.type": "io.debezium.transforms.outbox.EventRouter",
    "transforms.outbox.route.by.field": "nam_topic",
    "transforms.outbox.table.field.event.key": "txt_key",
    "transforms.outbox.table.field.event.payload": "txt_value",
    "transforms.outbox.table.fields.additional.placement": "txt_headers:header:x-headers-encoded,dat_created:header:dat-created",
    "transforms.outbox.table.event.id": "idt_tto_record",
    "transforms.outbox.table.event.timestamp": "dat_created",
    "transforms.extract.type": "org.apache.kafka.connect.transforms.ExtractField$Value",
    "transforms.extract.field": "txt_value",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.storage.ByteArrayConverter"
  }
}
